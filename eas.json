{
  "cli": {
    "version": ">= 5.0.0",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_PUBLIC_ENV": "preview",
          "EXPO_PUBLIC_BACKEND_URL": "@BACKEND_URL_PREVIEW"
        }
      }
    },
    "preview2": {
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_PUBLIC_ENV": "preview2"
        }
      }
    },
    "production": {
      "android": {
        "buildType": "aab",
        "env": {
          "EXPO_PUBLIC_ENV": "production",
          "EXPO_PUBLIC_BACKEND_URL": "@BACKEND_URL_PROD",
          "EXPO_PUBLIC_FIREBASE_API_KEY": "@FIREBASE_API_KEY",
          "EXPO_PUBLIC_FIREBASE_PROJECT_ID": "@FIREBASE_PROJECT_ID"
        }
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccount": "./firebase-key.json",
        "track": "internal"
      }
    }
  }
}
